{% extends "layout.html.twig" %}

{% block title %}Home{% endblock %}

{% block content %}
{% for event in events %}
<article>

    <h2><a class="eventTitle" href="{{ path('event', { 'id': event.num }) }}">{{ event.name }}</a></h2>
    <p>{{ event.desc }}</p>
</article>
{% endfor %}
{% endblock %}
